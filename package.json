{
    "name": "@bonapata/partes",
    "version": "0.2.3",
    "description": "Una librería libre de componentes para React, escrita en Typescript.",
    "main": "dist/cjs/index.js",
    "module": "dist/esm/index.js",
    "files": [
        "dist",
        "src"
    ],
    "devDependencies": {
        "@babel/core": "^7.18.6",
        "@babel/preset-env": "^7.18.6",
        "@babel/preset-react": "^7.18.6",
        "@cypress/react": "^6.0.0",
        "@types/cypress": "^1.1.3",
        "@types/jest": "^28.1.4",
        "@types/node": "^18.0.1",
        "@types/react": "^18.0.15",
        "@types/react-dom": "^17.0.11",
        "babel-loader": "^8.2.5",
        "cypress": "^10.3.0",
        "prettier": "^2.7.1",
        "react": "^18.2.0",
        "react-dom": "^18.2.0",
        "ts-jest": "^28.0.5",
        "typescript": "^4.7.4",
        "webpack": "^5.73.0"
    },
    "peerDependencies": {
        "react": "^16.9.0 || ^17.0.0 || ^18",
        "react-dom": "^16.9.0 || ^17.0.0 || ^18"
    },
    "scripts": {
        "add-hooks": "scripts/add-hooks.sh",
        "lint": "prettier --config .prettierrc --check ./",
        "lint:fix": "prettier --config .prettierrc --write ./",
        "prebuild": "npm run lint",
        "build": "rm -rf dist/ && npm run build:esm && npm run build:cjs",
        "build:esm": "tsc",
        "build:cjs": "tsc --module CommonJS --outDir dist/cjs",
        "pretest": "npm run build",
        "test": "cypress run --component",
        "test:open": "cypress open --component --browser chrome"
    },
    "repository": {
        "type": "git",
        "url": "git+https://github.com/juanmanuelgg/bonapata-partes.git"
    },
    "keywords": [
        "React",
        "components",
        "library",
        "libre"
    ],
    "author": "Juan Manuel González Garzón",
    "license": "Unlicense",
    "types": "./dist/cjs/index.d.ts",
    "bugs": {
        "url": "https://github.com/juanmanuelgg/bonapata-partes/issues"
    },
    "homepage": "https://github.com/juanmanuelgg/bonapata-partes#readme"
}
